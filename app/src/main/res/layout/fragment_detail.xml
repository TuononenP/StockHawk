<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.ContentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.udacity.stockhawk.ui.DetailActivityFragment">

    <LinearLayout
        android:id="@+id/stock_detail_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/stock_line_chart"
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>
</android.support.v7.widget.ContentFrameLayout>
